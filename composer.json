{
    "require": {
        "google/protobuf": "^3.20",
        "open-telemetry/opentelemetry": "^0.0.10",
        "twirp/twirp": "^0.8.1",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "guzzlehttp/psr7": "^2.2",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "datadog/php-datadogstatsd": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AppServer\\": "./src/AppServer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twitch\\": "example/Twitch"
        },
        "files": ["example/GPBMetadata/Example.php"]
    },
    "config": {
        "vendor-dir": "php-vendor"
    }
}
